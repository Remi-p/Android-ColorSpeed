<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate xmlns:android="http://schemas.android.com/apk/res/android"
            android:fromDegrees="-30"
            android:pivotX="0">
            <shape
                android:shape="line">
                <stroke
                    android:width="4dp"
                    android:color="#fff"/>
            </shape>
        </rotate>
    </item>
    <item>

        <rotate xmlns:android="http://schemas.android.com/apk/res/android"
            android:fromDegrees="30"
            android:pivotX="100%">
            <shape
                android:shape="line">
                <stroke
                    android:width="4dp"
                    android:color="#fff"/>
            </shape>
        </rotate>
    </item>
</layer-list>
